function f(day) {
    var result = 0;
    for (i = 0; i < arguments.length; i++) {
        if (typeof day  === 'number') {
         if ( day<= 7 && day>=1) {
            switch (day){
        case 1: 
            return("Monday");
            break;
        case 2: 
            return("Tuesday");
            break;
        case 3: 
            return("Wednesday");
            break;
        case 4: 
            return("Thursday");
            break;
        case 5: 
            return("Friday");
