let arr = [1, 2, -4, 3, -9, -1, 7];
function isPositive(num) {
for (i = 0; i < arguments.length; i++) {
    if (typeof num  === 'number') {
        if (num > 0) {
            return true
        } else {
            return false
          }
        }else {
            throw new Error ( ' Error: parameter type is not a Number' )
        }
    }
}
let newArr = [];
for (let i = 0; i < arr.length; i++) {
if (isPositive(arr[i])) {
newArr.push(arr[i]);
}
}
console.log(newArr);
